<%- include('../partials/header.ejs') %>

<h1>Stocks</h1>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Ticker</th>
            <th>Price</th>
            <th>Feeling</th>
        </tr>
    </thead>
    <tbody>
        <% for(let i = 0; i < stock.length; i++) {%>
            <tr>
                <td><%= stock[i].name %></td>
                <td><%= stock[i].ticker %></td>
                <td><%= stock[i].price %></td>
                <td><%= stock[i].feeling %></td>
            </tr>
        <%} %>
    </tbody>
</table>

<%- include('../partials/footer.ejs') %>
