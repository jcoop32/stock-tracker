<%- include('../partials/header.ejs') %>
<h3 class="mt-3">Add Stock</h3>
<div class="text-start">
  <div class="row align-items-start">
    <div class="col-6">
      <form action="/stocks" method="POST">
        <label
          >Stock Name:
          <input type="text" name="name" />
        </label>
        <br /><br />
        <label
          >Ticker:
          <input type="text" name="ticker" />
        </label>
        <br />
        <!-- <label
          >Price:
          <input type="number" step="0.01" name="price" />
        </label> -->
        <br />
        <label
          >Feeling:
          <select name="feeling">
            <option value="Bullish">Bullish</option>
            <option value="Bearish">Bearish</option>
          </select>
        </label>
        <br />
        <input value="Add Stock" class="btn btn-primary mt-3" type="submit" />
      </form>
    </div>
    <div class="col-6 mr-3">
      <div class="">
        <iframe src="https://datahub.io/core/nasdaq-listings/r/1.html" width="100%" height="450px" frameborder="0"></iframe>
      </div>
    </div>
  </div>
</div>




